{
  "$schema": "https://schemas.cute.engineering/stable/cutekit.manifest.target.v1",
  "id": "kobold-x86_64",
  "type": "target",
  "props": {
    "toolchain": "clang",
    "arch": "x86_64",
    "sys": "kobold",
    "abi": "sysv",
    "bits": "64",
    "freestanding": false,
    "host": false,
    "sys-encoding": "utf8",
    "sys-line-ending": "lf",
    "sys-path-separator": "slash",
    "sys-terminal": "ansi"
  },
  "tools": {
    "cc": {
      "cmd": "{shell.latest('clang')}",
      "args": [
        "--target=x86_64-none-elf"
      ]
    },
    "cxx": {
      "cmd": "{shell.latest('clang++')}",
      "args": [
        "--target=x86_64-none-elf",
        "-fno-exceptions",
        "-fno-rtti"
      ]
    }
  }
}